<div class="container">
    {{#each posts as |post| }}

    <div>
        <div>
            <h2>
                <a href="/post/{{post.id}}">{{post.title}}</a>
            </h2>
            
        </div>
        <div>
            <p>
                {{post.content}}
            </p>
        </div>
    </div>
    {{/each}}
</div>